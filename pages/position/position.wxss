/* pages/position/position.wxss */
.bg{
  position: fixed;
  width:100%;
  height:100%;
  top:0rpx;
  z-index:-1;
}

.up{
  position: fixed;
  display: flex;
  background:url('data:image/png;base64,');
  background-size:750rpx,70rpx;
  width:720rpx;
  height:70rpx;
  line-height: 70rpx;
  font-size:30rpx;
  padding-top:20rpx;
  padding-bottom:20rpx;
  padding-left:30rpx;
  z-index:10;
  top:0rpx;
}

.search{
  display: flex;
  background:#ffffff;
  border-radius: 10rpx;
}

.search-icon{
  width:44rpx;
  height:44rpx;
  margin-top:13rpx;
  margin-left:13rpx;
}

.search-input{
  width:420rpx;
  height:70rpx;
  padding-left:15rpx;
  font-size:30rpx;
}

.icon-container{
  width:70rpx;
  height:70rpx;
}

.close{
  width:30rpx;
  height:30rpx;
  margin-top:20rpx;
  margin-left:20rpx;
}

.cancel{
  width:140rpx;
  text-align: center;
}

.list-item{
  display: flex;
  justify-content: space-between;
  width:720rpx;
  padding-left:30rpx;
  border-bottom:1rpx solid rgba(94, 94, 94,0.1);
  height:80rpx;
  font-size:30rpx;
  line-height:80rpx;
}

.list-history{
  font-size:22rpx;
  height:40rpx;
  line-height:40rpx;
  margin-top:20rpx;
  margin-right:30rpx;
  background: rgb(65, 125, 255);
  padding-left:10rpx;
  padding-right:10rpx;
  border-radius: 10rpx;
  color:white;
}

/* 快捷查找 */
.quick{
  padding-left:15rpx;
  padding-right:15rpx;
}

.city{
  float: left;
  width:210rpx;
  height:70rpx;
  line-height: 70rpx;
  /* background:#f2f2f2; */
  color:#676767;
  text-align: center;
  margin-left:15rpx;
  margin-right:15rpx;
  margin-top:30rpx;
  font-size:25rpx;
  border-radius: 15rpx;
}

/* 搜索栏 */
.scroll{
  width:100%;
  background: rgba(255, 255, 255,1);
  position: fixed;
  top:110rpx;
}

/* 历史记录 */
.history{
  display: flex;
  justify-content: space-between;
  width:690rpx;
  padding-left:30rpx;
  padding-right:30rpx;
  height:80rpx;
  line-height: 80rpx;
  font-size:35rpx;
  margin-top:20rpx;
}

.history-right{
  display: flex;
  padding-left:20rpx;
}

.history-icon{
  width:50rpx;
  height:50rpx;
  margin-top:15rpx;
}

.select1{
  background: rgba(126, 126, 126, 0.1);
}

.select2{
  opacity: 0.3;
}